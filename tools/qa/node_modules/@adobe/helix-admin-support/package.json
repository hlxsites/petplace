{
  "name": "@adobe/helix-admin-support",
  "version": "2.1.17",
  "description": "Helix Admin Support Library",
  "main": "src/index.js",
  "type": "module",
  "types": "src/index.d.ts",
  "scripts": {
    "test": "c8 mocha",
    "lint": "eslint .",
    "docs:schema": "jsonschema2md -d src/schemas -o docs -x docs -f yaml",
    "docs:api": "jsdoc2md -c .jsdoc.json --files src/*.js > docs/API.md",
    "docs:types": "node ./test/dev/generate-types.js",
    "docs": "npm run docs:schema && npm run docs:api && npm run docs:types",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-admin-support"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-admin-support/issues"
  },
  "homepage": "https://github.com/adobe/helix-admin-support#readme",
  "mocha": {
    "require": [
      "test/setup-env.js"
    ],
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "dependencies": {
    "@adobe/fetch": "4.0.13",
    "@adobe/helix-shared-config": "10.2.1",
    "@adobe/helix-shared-utils": "3.0.0",
    "@aws-sdk/client-s3": "3.335.0",
    "@aws-sdk/client-sqs": "3.335.0",
    "@aws-sdk/node-http-handler": "3.329.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "2.0.2",
    "@adobe/jsonschema2md": "7.1.5",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "10.0.3",
    "c8": "7.13.0",
    "eslint": "8.41.0",
    "husky": "8.0.3",
    "jsdoc-to-markdown": "8.0.0",
    "json-schema-to-typescript": "13.0.1",
    "junit-report-builder": "3.0.1",
    "lint-staged": "13.2.2",
    "mocha": "10.2.0",
    "mocha-multi-reporters": "1.5.1",
    "nock": "13.3.1",
    "semantic-release": "21.0.2",
    "yaml": "2.2.2"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.cjs": "eslint"
  }
}
